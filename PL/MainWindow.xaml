<Window x:Class="PL.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PL"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="btnAdmin" Content="Admin" HorizontalAlignment="Left" Margin="96,116,0,0" VerticalAlignment="Top" Background="#FFF1811F" Style="{StaticResource MainWindowButtons}" Click="btnAdmin_Click"/>
        <Button x:Name="btnNewOrder" Content="New Order" HorizontalAlignment="Left" Margin="96,266,0,0" VerticalAlignment="Top" Click="Button_Click" Background="#FF220ACF" Style="{StaticResource MainWindowButtons}"/>
        <TextBox x:Name="txtbxOrderNo" HorizontalAlignment="Left" Height="32" Margin="565,116,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="184"/>
        <Button x:Name="btnTrack" Content="Track" HorizontalAlignment="Left" Margin="565,161,0,0" VerticalAlignment="Top" Style="{StaticResource MainWindowButtons}"/>

    </Grid>
</Window>
